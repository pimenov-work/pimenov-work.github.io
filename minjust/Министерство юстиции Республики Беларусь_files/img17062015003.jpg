      <script>
        $(document).ready(function () {
          var submit_login_form = function () {
            var button = $(this);
            if (button.prop('disabled'))
              return;
            $(".login .login-form, .login input").removeClass('error');
            var form = $('#top_login_form');
            var form_el = $('#top_login_form').get(0);
            var names = ['username', 'password'];
            for (var i = 0; i < names.length; ++i) {
              var name = names[i];
              if (!form_el.elements[name].value) {
                $(form_el.elements[name]).addClass('error').focus();
                break;
              }
            }
            $.ajax({
              url: '/ru/account/ajax/login/',
              data: form.serialize(),
              type: 'post',
              success: function (output) {
                if (output) {
                  if (output.errors) {
                    var errors = output.errors;
                    if (errors.__all__ && errors.__all__[0]) {
                      $('.login .login-form .error-message').html(errors.__all__[0]);
                      $('.login .login-form').addClass('error');
                      return;
                    }
                    for (var i = 0; i < names.length; ++i) {
                      var name = names[i];
                      if (errors[name]) {
                        var input_element = form_el.elements[name];
                        var error_message = input_element.placeholder + ' - ' + errors[name];
                        $(input_element).addClass('error').focus();
                        $('.login .login-form .error-message').html(error_message);
                        return;
                      }
                    }
                  }
                  if (output.next_url) {
                    window.location.href = output.next_url;
                  }
                }
              },
              error: function () {
                window.location.href = '/ru/account/login/';
              }
            }).done(function () {
              button.prop('disabled', false);
            });
            button.prop('disabled', true);
          };
          $('.login .button').click(submit_login_form);
          $("#top_login_form").on("keypress", function (event) {
            if (event.which == 13 && !event.shiftKey) {
              event.preventDefault();
              submit_login_form();
            }
          });
          $("#top_login_form .submit-emulator").on("keypress", function (event) {
            if ((event.which == 13 || event.which == 32) && !event.shiftKey) {
              event.preventDefault();
              submit_login_form();
            }
          }).on("click", function (e) {
            event.preventDefault();
            submit_login_form();
          });
        });
      </script>
    
