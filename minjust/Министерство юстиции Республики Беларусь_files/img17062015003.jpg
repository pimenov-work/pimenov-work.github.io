        <script type="text/javascript">
            // Voting Data
            var voteResultData = [
                { y: 2,    n: 100, u: 8 },
                { y: 32, n: 50, u: 18 },
                { y: 40, n: 40, u: 20 }
            ]
            // Init Voting
            function voteInitialize() {
                var results  = $('.interview .voting-results');
                var totalW = $('.interview .voting-results .bar').outerWidth(false);
                for (var i in voteResultData) {
                    $(results[i]).find('#yes .count').text(voteResultData[i].y + '% (' + voteResultData[i].y + ')');
                    $(results[i]).find('#no .count').text(voteResultData[i].n + '% (' + voteResultData[i].n + ')');
                    $(results[i]).find('#undefined .count').text(voteResultData[i].u + '% (' + voteResultData[i].u + ')');

                    $(results[i]).find('#yes .fill').width(totalW / 100 * voteResultData[i].y);
                    $(results[i]).find('#no .fill').width(totalW / 100 * voteResultData[i].n);
                    $(results[i]).find('#undefined .fill').width(totalW / 100 * voteResultData[i].u);
                }
            }
            $(document).ready(function(){
                // Init Voting
                voteInitialize();
                // Main Slider
                $('.main-slider .item').hover(function(){
                    $('.main-slider .item').removeClass('active');
                    $(this).addClass('active');
                });
                // Search Checkboxes

                $('.voting-holder .checkbox').click(function() {
                    var parent = $(this).parents('.content-item');
                    $('.voting-holder .checkbox').removeClass('checked');
                    $(this).addClass('checked');
                    setTimeout(function() {
                        parent.find('.voting-holder').addClass('hide');
                        parent.find('.voting-results').addClass('show');
                    }, 500);
                });
            });
        </script>
